{
  "name": "@univerjs/webpack-plugin",
  "version": "0.2.6",
  "private": false,
  "description": "Webpack plugin for easily integrating Univer",
  "author": "DreamNum <developer@univer.ai>",
  "license": "Apache-2.0",
  "keywords": [
    "univer",
    "univerjs",
