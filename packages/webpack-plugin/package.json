    "@univerjs/plugin-core": "workspace:*",
    "@univerjs/ui": "0.1.0-beta.5",
    "css-loader": "^6.10.0",
    "esbuild": "^0.20.1",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
